{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "HLmsp",
	"description": "Structure for MSP definitions supported by Hyperledger",
	"type": "object",
	"properties": {
		"Organization": {
			"description": "The MSP organization name",
			"type": "string"
		},
		"OrganizationUnit": {
			"type": "string"
		},
		"MSP": {
			"type": "string"
		},
		"X509Certificate": {
			"type": "string"
		}
	},
	"required": [
		"Organization",
		"X509Certificate"
	]
}